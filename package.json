		"builtin-modules": "^5.0.0",
		"esbuild": "^0.27.0",
		"eslint": "^9.38.0",
		"obsidian": "latest",
		"tslib": "^2.8.1",
		"typescript": "^5.9.3"
	},
	"dependencies": {
		"date-fns": "^4.1.0",
